{
  "name": "time-calc",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "concurrently": "^3.2.0",
    "node-sass": "^4.5.0",
    "react-scripts": "^0.9",
    "sw-precache": "^5.0.0"
  },
  "dependencies": {
    "csvwriter": "^1.3.0",
    "isomorphic-fetch": "^2.2.1",
    "material-design-lite": "^1.3.0",
    "moment": "^2.17.1",
    "moment-duration-format": "https://github.com/jsmreese/moment-duration-format/archive/dev.tar.gz",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-forms": "beta",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0",
    "store": "^2.0.4"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch",
    "dev": "concurrently \"cd server && npm run dev\" \"npm run client:start\" \"npm run watch-css\"",
    "start": "npm run client:build && npm start --prefix server",
    "client:start": "react-scripts start",
    "client:build": "mv .env ._env && concurrently \"react-scripts build\" \"npm run build-css\" && sw-precache --config=sw-precache-config.js && mv ._env .env",
    "client:test": "react-scripts test --env=jsdom",
    "client:eject": "react-scripts eject",
    "deploy": "cd server && npm run deploy"
  }
}
