{
  "name": "time-calc",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-watch": "^2.0.6",
    "concurrently": "^3.2.0",
    "node-sass": "^4.5.0",
    "react-scripts": "^0.9"
  },
  "dependencies": {
    "boom": "^4.2.0",
    "bottlejs": "^1.5.0",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.0",
    "koa-context-validator": "^0.3.0",
    "koa-cors": "^0.0.16",
    "koa-logger": "next",
    "koa-onerror": "^3.0.1",
    "koa-router": "next",
    "material-design-lite": "^1.3.0",
    "moment": "^2.17.1",
    "moment-duration-format": "https://github.com/jsmreese/moment-duration-format/archive/dev.tar.gz",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-forms": "beta",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch",
    "start": "concurrently \"npm run server:start\" \"npm run client:start\"",
    "server:start": "babel-watch server/index.js",
    "client:start": "react-scripts start & npm run watch-css",
    "client:build": "react-scripts build && npm run build-css",
    "client:test": "react-scripts test --env=jsdom",
    "client:eject": "react-scripts eject"
  }
}
